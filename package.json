    "@effect/vitest": "^0.20.0",
    "@types/node": "^22.9.0",
    "commitlint": "^19.5.0",
    "esbuild": "^0.25.0",
    "lefthook": "^1.8.2",
    "node-html-parser": "^7.0.0",
    "prisma": "^6.0.0",
    "ts-pattern": "^5.5.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^3.0.0"
  },
  "dependencies": {
    "@effect/platform": "^0.80.0",
    "@effect/platform-node": "^0.76.0",
    "@prisma/client": "6.6.0",
    "discord.js": "^14.16.3",
