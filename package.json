{
  "name": "AtCoDaily-bot",
  "version": "0.1.0",
  "description": "Post AtCoder problem everyday in Discord",
  "main": "index.js",
  "scripts": {
    "build": "tsx build.ts",
    "check:type": "tsc --noEmit",
    "check:format": "biome format ./src",
    "format": "biome format --write ./src",
    "check:lint": "biome check ./src",
    "lint": "biome check --write ./src",
    "test": "vitest",
    "postinstall": "lefthook install",
    "dev": "tsx --env-file=.env src/main.ts"
  },
  "type": "module",
  "keywords": [],
